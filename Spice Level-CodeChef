import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc=new Scanner(System.in);
		int T=sc.nextInt();
		while(T-->0){
		    int X=sc.nextInt();
		    if(X<4){
		        System.out.println("Mild");
		    }else if(X>=4&&X<7){
		        System.out.println("Medium");
		    }else{
		        System.out.println("Hot");
		    }
		}

	}
}
