import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc=new Scanner(System.in);
		int X=sc.nextInt();
		int N=sc.nextInt();
		int M=sc.nextInt();
		if(N<=X+M){
		    System.out.println("YES");
		}else{
		    System.out.println("NO");
		}

	}
}
