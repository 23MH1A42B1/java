import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc=new Scanner(System.in);
		int T=sc.nextInt();
		while(T-->0){
		    int X=sc.nextInt();
		    if(X<=3){
		        System.out.println("BRONZE");
		    }else if(X>3&&X<=6){
		        System.out.println("SILVER");
		    }else{
		        System.out.println("GOLD");
		    }
		}

	}
}
